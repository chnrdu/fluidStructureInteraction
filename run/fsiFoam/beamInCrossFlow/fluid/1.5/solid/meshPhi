FoamFile
{
    version     2.0;
    format      ascii;
    class       surfaceScalarField;
    location    "1.5";
    object      meshPhi;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 3 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    interface
    {
        type            calculated;
        value           uniform 0;
    }
    symmetry
    {
        type            symmetryPlane;
    }
    bottom
    {
        type            calculated;
        value           uniform 0;
    }
}


