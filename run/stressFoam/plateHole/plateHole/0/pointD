FoamFile
{
    version     2.0;
    format      ascii;
    class       pointVectorField;
    object      pointD;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 0 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    left
    {
        type symmetryPlane;
    }
    right
    {
        type calculated;
    }
    down
    {
        type symmetryPlane;
    }
    up
    {
        type calculated;
    }
    hole
    {
        type calculated;
    }
    frontAndBack
    {
        type empty;
    }
}

