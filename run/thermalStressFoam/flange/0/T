FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      pointD;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 273;

boundaryField
{
    patch1
    {
        type            zeroGradient;
    }

    patch2
    {
        type            fixedValue;
        value           uniform 273;
    }

    patch3
    {
        type            zeroGradient;
    }

    patch4
    {
        type            fixedValue;
        value           uniform 573;
    }
}

